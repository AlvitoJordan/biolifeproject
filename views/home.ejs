    <script>
        //Fungsi pencarian jika dalam mode web
        function search () {
            let cari = document.getElementById("cari").value;
            axios({
                method: 'get',
                url: '/search/' + cari,
            })
            .then((result) => {
                window.location = "/search/" + cari;
            }).catch((err) => {
                window.location = "/";
            })
        }
        //Fungsi pencarian jika dalam mode mobile
        function MobileSearch() {
            let SearchM = document.getElementById("SearchMobile").value;
            axios({
                method: 'get',
                url: '/search/' + SearchM,
            })
            .then((result) => {
                window.location = "/search/" + SearchM;
            }).catch((err) => {
                window.location = "/";
            })
        }
        //Fungsi Wish dan Unwish
        function Wish(id) {
            axios({
                method: 'post',
                url: '/wishlist/' + id,
            })
            .then((result) => {
                alert("Success!")
                location.reload()
            }).catch((err) => {
                alert("ERROR")
                window.location = "/";
            })
        }
        //Fungsi Add New Cart dan Update cart
        function addCart(a,b){
            axios({
                method: 'post',
                url : '/addcart/' + a,
                data : {
                    ProductProductId : a,
                    Qty : 1,
                    Price : b,
                }  
            }).then(() => {
                alert("Success!")
                location.reload()
            }).catch((err) => {
                alert("ERROR")
            })
        }
    </script>
