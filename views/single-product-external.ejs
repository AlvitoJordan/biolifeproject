    <script>
        //Fungsi Search untuk web
        function search () {
            let cari = document.getElementById("cari").value;
            axios({
                method: 'get',
                url: '/search/' + cari,
            })
            .then((result) => {
                window.location = "/search/" + cari;
            }).catch((err) => {
                window.location = "/";
            })
        }
        //Fungsi Search untuk mobile
        function MobileSearch() {
            let SearchM = document.getElementById("SearchMobile").value;
            axios({
                method: 'get',
                url: '/search/' + SearchM,
            })
            .then((result) => {
                window.location = "/search/" + SearchM;
            }).catch((err) => {
                window.location = "/";
            })
        }
        //Fungsi add Cart pada produk yang sedang dilihat
        function addCart(a,b){
            console.log(document.getElementById("carttt").value)
            axios({
                method: 'post',
                url : '/addcart/' + a,
                data : {
                    ProductProductId : a,
                    Qty : parseInt(document.getElementById("carttt").value,10),
                    Price : b,
                }  
            })
            .then(() => {
                alert("Success!")
                location.reload()
            }).catch((err) => {
                alert("ERROR")
            })
        }
        //Fungsi add Cart untuk Related Product
        function addOtherCart(a,b){
