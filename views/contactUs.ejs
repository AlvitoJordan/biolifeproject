    <script>
        //Fungsi pencarian jika dalam mode web
        function search () {
            let cari = document.getElementById("cari").value;
            axios({
                method: 'get',
                url: '/search/' + cari,
            })
            .then((result) => {
                window.location = "/search/" + cari;
            }).catch((err) => {
                window.location = "/";
            })
        }
        //Fungsi pencarian jika dalam mode mobile
        function MobileSearch() {
            let SearchM = document.getElementById("SearchMobile").value;
            axios({
                method: 'get',
                url: '/search/' + SearchM,
            })
            .then((result) => {
                window.location = "/search/" + SearchM;
            }).catch((err) => {
                window.location = "/";
            })
        }

        //Fungsi kirim Kontak 
        document.getElementById('save').onclick = () => {
            axios({
                method: 'post',
                url: '/api/contactUs',
                data: {
                    comment: document.getElementById("comment").value
                }
            }).then((result) => {
                alert("Thank You for Contacting Biolife, We will process your complaint")
                window.location = "/contactus";
            }).catch((err) => {
                alert('error ' + err)
            });
        }
    </script>
